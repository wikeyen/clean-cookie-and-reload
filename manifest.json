{
  "manifest_version": 2,
  "name": "Reload Page Without Cookies",
  "description": "Clear Cookie for current tab and reload",
  "version": "0.1",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "script/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "resource/icon/cookie16.png",
    "32": "resource/icon/cookie32.png",
    "48": "resource/icon/cookie48.png",
    "128": "resource/icon/cookie128.png"
  },
  "browser_action": {
    "default_icon": "resource/icon/cookie16.png",
    "default_title": "Reload Page Without Cookies"
  },
  "author": "Mike Yan"
}